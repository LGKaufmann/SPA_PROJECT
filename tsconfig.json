{
    "compilerOptions": {
      "target": "es2015",
      "module": "commonjs",
      "jsx": "react-jsx",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*", "@types/**/*"], // Incluye la carpeta @types
    "exclude": ["node_modules"]
  }
  